# S3 Configuration for public read access
# These are optional - if not set, only admin mode will work

# VK Cloud S3 Endpoint
VITE_S3_ENDPOINT=https://hb.vkcloud-storage.ru

# S3 Region
VITE_S3_REGION=ru-msk

# Public bucket name (must have public read access configured)
VITE_PUBLIC_BUCKET=bll01

# Admin token format (stored in SecureStorage, not here):
# accessKeyId:secretAccessKey:endpoint:region:bucket
